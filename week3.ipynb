    "import pandas as pd\n",
    "import numpy as np\n",
    "x = 0\n",
    "cumulative_miles = 0\n",
    "while x != 10:\n",
    "    D_miles = pd.Series(np.random.randint(55,65))\n",
    "    cumulative_miles = cumulative_miles + D_miles\n",
    "    x += 1\n",
    "    print('Day{} cumulative miles:{}'.format(x,cumulative_miles))"
   ]
